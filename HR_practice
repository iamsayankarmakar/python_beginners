if __name__ == '__main__':
    n = int(input())
    student_marks = {}
    for _ in range(n):
        name, *line = input().split()
        scores = list(map(float, line))
        student_marks[name] = scores
    query_name = input()
    #print(student_marks)
    #print(student_marks[query_name])
    #mq = student_marks[query_name]
    #print(mq)
    sum = 0
    for m in student_marks[query_name]:
        sum += m
        avg = sum/3
    print(format(avg, ".2f"))